    <x:expect label="generate 036 from bf:StudyNumber" test="count(//marc:datafield[@tag='036']) = 1"/>
    <x:expect label="rdf:value goes in $a" test="//marc:datafield[@tag='036']/marc:subfield[@code='a'] = 'CNRS 84115'"/>
